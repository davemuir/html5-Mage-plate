@media only screen 
and (max-width: 1024px) and (min-width:480px) {
	nav{
		width:auto;
		height:38px;
		
	

		}
		nav ul{
			list-style-type: none;
			padding:0;
			margin:0;
			height:100%;
			text-align:center;
		}
		nav ul li:hover{
					background-image:url(img/hover-dark.png);
					
				}	
		nav ul li {
			background: #898989; /* Old browsers */
			background: -moz-linear-gradient(top,  #898989 7%, #898989 15%, #8b8b8b 35%, #747474 51%, #666666 51%, #575757 89%); /* FF3.6+ */
			background: -webkit-gradient(linear, left top, left bottom, color-stop(7%,#898989), color-stop(15%,#898989), color-stop(35%,#8b8b8b), color-stop(51%,#747474), color-stop(51%,#666666), color-stop(89%,#575757)); /* Chrome,Safari4+ */
			background: -webkit-linear-gradient(top,  #898989 7%,#898989 15%,#8b8b8b 35%,#747474 51%,#666666 51%,#575757 89%); /* Chrome10+,Safari5.1+ */
			background: -o-linear-gradient(top,  #898989 7%,#898989 15%,#8b8b8b 35%,#747474 51%,#666666 51%,#575757 89%); /* Opera 11.10+ */
			background: -ms-linear-gradient(top,  #898989 7%,#898989 15%,#8b8b8b 35%,#747474 51%,#666666 51%,#575757 89%); /* IE10+ */
			background: linear-gradient(to bottom,  #898989 7%,#898989 15%,#8b8b8b 35%,#747474 51%,#666666 51%,#575757 89%); /* W3C */
			filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#898989', endColorstr='#575757',GradientType=0 ); /* IE6-9 */
				
				display:inline;
				border-left:1px solid #BFBFBF;
				padding-top:10px;
				padding-bottom:10px;
				padding-left:20px;
				padding-right:20px;
				
				border:1px solid #BFBFBF;
				a{
					color:white;
					text-shadow:0px 0px 3px grey !important;
					text-decoration:none;
				}
			
				
		}
		nav ul li.first{
	    border-left:none;
	    width:100%;
		height:100%;
		border-radius:10px 0px 0px 10px;
		}
		
		nav ul li.last{
	    border-left:none;
	    width:100%;
		height:100%;
		border-radius:0px 10px 10px 0px;
		}

			

	
	#header{
		text-align:center;
		width:600px;
		margin: auto;
	}
	h1{
		color:black;
	}
	#mainContainer{
		width:600px;
		margin:auto;
	}
}




@media only screen
and (max-width: 480px) and (min-width:320px) {

	html {
		-webkit-text-size-adjust: none;
	}
	body{
		background-image:url(http://html5mageplate.herokuapp.com/img/pinstripe-apple.jpg);
	}
	#mainContainer{
		width:300px;
		height:auto;
		margin-left:auto;
		margin-right:auto;
	}
	#header{
		background: #cccccc; /* Old browsers */
		background: -moz-linear-gradient(top,  #cccccc 0%, #cccccc 0%, #4c6d99 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#cccccc), color-stop(0%,#cccccc), color-stop(100%,#4c6d99)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #cccccc 0%,#cccccc 0%,#4c6d99 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  #cccccc 0%,#cccccc 0%,#4c6d99 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  #cccccc 0%,#cccccc 0%,#4c6d99 100%); /* IE10+ */
		background: linear-gradient(to bottom,  #cccccc 0%,#cccccc 0%,#4c6d99 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#cccccc', endColorstr='#4c6d99',GradientType=0 ); /* IE6-9 */

			width:100%;
			margin:0;
			padding:0;
			float:top;    
			border-bottom:1px solid rgba(15,19,58,0.96);
		 
		h1{
		color:white;
		text-shadow:0px 0px 3px grey;
		text-align:center;
		margin:0;
		padding:0px;
		text-decoration: none;
		}	

	}
	nav{
		border:none;
	}
	nav ul a {
		font-size: 90%;
		padding: 10px 8px;
		color:black !important;
	}
	nav ul{
		list-style-type: none;
		margin:10px 0px 0px 0px;
		padding:0;
	}
	nav ul li.last{
		border-bottom:1px solid #BFBFBF;
		border-left:1px solid #BFBFBF;
		border-right:1px solid #BFBFBF;
		border-top:none;
		border-radius:0px 0px 10px 10px;
	}
	nav ul li.first{
		border-top:1px solid #BFBFBF;
		border-left:1px solid #BFBFBF;
		border-right:1px solid #BFBFBF;
		border-bottom:none;
		border-radius: 10px 10px 0px 0px;

	}
	nav ul li {
		border:1px solid #BFBFBF;
		display:inherit;
		text-align:center;
		padding:5px;
		background-color:white;
	}
}



/* ==========================================================================
   Print styles.
   Inlined to avoid required HTTP connection: h5bp.com/r
   ========================================================================== */

@media print {
    * {
        background: transparent !important;
        color: #000 !important; /* Black prints faster: h5bp.com/s */
        box-shadow: none !important;
        text-shadow: none !important;
    }

    a,
    a:visited {
        text-decoration: underline;
    }

    a[href]:after {
        content: " (" attr(href) ")";
    }

    abbr[title]:after {
        content: " (" attr(title) ")";
    }

    /*
     * Don't show links for images, or javascript/internal links
     */

    .ir a:after,
    a[href^="javascript:"]:after,
    a[href^="#"]:after {
        content: "";
    }

    pre,
    blockquote {
        border: 1px solid #999;
        page-break-inside: avoid;
    }

    thead {
        display: table-header-group; /* h5bp.com/t */
    }

    tr,
    img {
        page-break-inside: avoid;
    }

    img {
        max-width: 100% !important;
    }

    @page {
        margin: 0.5cm;
    }

    p,
    h2,
    h3 {
        orphans: 3;
        widows: 3;
    }

    h2,
    h3 {
        page-break-after: avoid;
    }
}
